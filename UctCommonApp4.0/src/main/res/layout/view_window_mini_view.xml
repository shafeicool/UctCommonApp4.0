<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/x100"
    android:layout_height="@dimen/y120"
    android:background="@drawable/shape_round_corner_stroke_blue"
    android:id="@+id/rl_miniParent"
    android:visibility="gone">
    <ImageView
        android:id="@+id/iv_type"
        android:layout_width="@dimen/y50"
        android:layout_height="@dimen/y50"
        android:src="@mipmap/logo"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/y20"/>
    <TextView
        android:id="@+id/tv_speakTime"
        style="@style/tv2et_wrap_content_12sp"
        android:text="组呼中"
        android:textColor="@color/colorBlue"
        android:layout_below="@+id/iv_type"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/y6"/>
</RelativeLayout>